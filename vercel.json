{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/dashboard",
      "destination": "/academy",
      "permanent": true
    },
    {
      "source": "/dashboard.html",
      "destination": "/academy",
      "permanent": true
    },
    {
      "source": "/landing",
      "destination": "/ebook",
      "permanent": true
    },
    {
      "source": "/checkout",
      "destination": "/wacht-even",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/academy",
      "destination": "/academy.html"
    },
    {
      "source": "/ebook",
      "destination": "/landing.html"
    },
    {
      "source": "/wacht-even",
      "destination": "/checkout.html"
    }
  ]
}
